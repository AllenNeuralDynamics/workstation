<process name="MCFO 40x Alignment Pipeline" processor="queue/samplePipelineLauncher" startEvent="pending" updateProcessStatus="on_success">
    <sequence>

        <include name="Pipeline" process="FlyLightSamplePipeline">

            <input name="OBJECTIVE" value="40x"/>
            <input name="PIPELINE_PROCESS" value="MCFO40x"/>
            <input name="PIPELINE_NAME" value="MCFO 40x Pipeline"/>
            
            <input name="CHANNEL_DYE_SPEC" value="reference=Alexa Fluor 488,Cy2;membrane_ha=,ATTO 647,Alexa Fluor 633,Alexa Fluor 647,Cy5;membrane_v5=Alexa Fluor 546,Alexa Fluor 555,Alexa Fluor 568,DY-547,Cy3;membrane_flag=Alexa Fluor 594"/>
            <input name="OUTPUT_CHANNEL_ORDER" value="membrane_ha,membrane_v5,membrane_flag,reference"/>
            <input name="OUTPUT_COLOR_SPEC" value="RGB1"/>
            <input name="IMAGE_TYPE" value="mcfo"/>
            
            <input name="MERGE_ALGORITHMS" value="FLYLIGHT_ORDERED"/>
            <input name="STITCH_ALGORITHMS" value="FLYLIGHT"/>

            <input name="ALIGNMENT_ALGORITHMS" value="aligner_vnc2017_20x,aligner_vnc2017_20x,aligner_yuy"/>
            <input name="ALIGNMENT_ALGORITHM_PARAMS" value="align,align_female,brainalign40xMCFO_INT"/>
            <input name="ALIGNMENT_ALGORITHM_RESULT_NAMES" value="VNC Alignment,VNC Mask Alignment,Brain Alignment"/>

            <input name="ANALYSIS_ALGORITHMS" value="NEURON_SEPARATOR"/>
            
        </include>

    </sequence>
</process>